<h1>Listing food_types</h1>

<table>
  <tr>
    <th>Content</th>
    <th>Yes</th>
    <th>updated</th>
    <th></th>
    <th></th>
  </tr>
  
<% @food_types = FoodType.alphabetical %>
<% @food_types.each do |food_type| %>
  <tr>
    <td><%= food_type.content %></td>
    <td><%= food_type.yes %></td>
    <td><%= food_type.updated_at %></td>
    <td><%= link_to 'Show', food_type %></td>
    <td><%= link_to 'Edit', edit_food_type_path(food_type) %></td>
    <td><%= link_to 'Destroy', food_type, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Food type', new_food_type_path %>
